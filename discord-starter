import random
import string

min=1
max=2
marco = ""

x=string.ascii_uppercase

full_list = ""
for i in x:
    full_list += i

marco = ""

while len(marco) <= max:
    marco += random.choice(full_list)
print(marco)

import random
import string

min=1
max=2
margo = ""

x=string.ascii_lowercase

full_list = ""
for i in x:
    full_list += i

margo = ""

while len(margo) <= max:
    margo += random.choice(full_list)
print(margo)

